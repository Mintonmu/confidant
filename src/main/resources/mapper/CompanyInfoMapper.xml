<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzr.confidant.mapper.CompanyInfoMapper">

    <update id="saveCompanyDescribe" parameterType="java.lang.String">
        update companyinfo
        set companyDescribe=#{companyDescribe}
        where id=#{companyId}
    </update>

</mapper>